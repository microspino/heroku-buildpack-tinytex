#!/usr/bin/env bash
# bin/compile <build-dir> <cache-dir>

set -e

BUILD_DIR=$1
CACHE_DIR=$2
BINDIR=$(cd $(dirname $0); pwd)
PLATFORM=$(uname -m)-$(uname -s | tr A-Z a-z)

source $BINDIR/utils

cd ./bin
./install.sh

ls -al

export PATH=$BINDIR:$PATH

if [ ! `which pdflatex` ]; then
    build-warn "Cannot find TinyTex installation failed"
    exit 1
fi


